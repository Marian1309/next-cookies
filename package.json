{
  "name": "@pidchashyi/next-cookies",
  "version": "1.2.0",
  "description": "A utility to get cookies from Next.js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./v15": {
      "import": "./dist/v15/index.js",
      "require": "./dist/v15/index.js",
      "types": "./dist/v15/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build"
  },
  "keywords": [
    "next",
    "cookies",
    "typescript",
    "utility"
  ],
  "author": "Marian Pidchashyi",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^24.0.3",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "next-14": "npm:next@14.2.15",
    "next-15": "npm:next@15.3.0"
  }
}
